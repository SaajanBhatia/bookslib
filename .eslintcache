           .doc(id)\r\n            .update({\r\n              authors: firebase.firestore.FieldValue.arrayUnion({\r\n                ...author,\r\n                photo: downloadURL,\r\n              }),\r\n            });\r\n          addToast({ text: \"Author added successfully\", type: \"success\" });\r\n          setLoading(false);\r\n          dispatch({\r\n            type: \"addAuthor\",\r\n            author: { ...author, photo: downloadURL },\r\n          });\r\n          setAuthor({\r\n            name: \"\",\r\n            photo: \"\",\r\n            description: \"\",\r\n          });\r\n        }\r\n      );\r\n    } catch (e) {\r\n      console.error(\"An error has occured while adding an author\", e);\r\n      setError(\"Unable to add author.\");\r\n      setLoading(false);\r\n    }\r\n  };\r\n  return (\r\n    <>\r\n      <h3>Authors</h3>\r\n      <GridDiv>\r\n        {book.authors\r\n          ? book.authors.map((author, i) => (\r\n              <BookAuthor key={author.name+i} {...{ author, id, dispatch }} />\r\n            ))\r\n          : \"No authors\"}\r\n      </GridDiv>\r\n      <Divider />\r\n      <div>\r\n        <h4>Add an author</h4>\r\n        <AuthorForm\r\n          {...{ loading, error, author, setAuthor }}\r\n          onSubmit={onSubmit.bind(this, author)}\r\n        />\r\n      </div>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default BookAuthors;\r\n","C:\\Users\\salim\\projects\\youtube\\react\\bookslib\\src\\index.js",[],["187","188"],"C:\\Users\\salim\\projects\\youtube\\react\\bookslib\\src\\components\\book-details\\BookMenu.js",[],["189","190"],"C:\\Users\\salim\\projects\\youtube\\react\\bookslib\\src\\components\\BookItem.js",[],["191","192"],"C:\\Users\\salim\\projects\\youtube\\react\\bookslib\\src\\components\\AddBook.js",[],["193","194"],"C:\\Users\\salim\\projects\\youtube\\react\\bookslib\\src\\components\\AuthorForm.js",["195"],"import { Field, Message, Button } from \"../ui\";\r\nimport profileImg from \"../profile-pla